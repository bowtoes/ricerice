#!/usr/bin/env sh
types=""
types="${types}\|ani"

types="${types}\|bsp"

types="${types}\|cfg"
types="${types}\|cnf"
types="${types}\|conf"
types="${types}\|config"
types="${types}\|cur"

types="${types}\|dll"

types="${types}\|fnt"
types="${types}\|fon"

types="${types}\|ini"
types="${types}\|inf"

types="${types}\|jpg"
types="${types}\|jpeg"

types="${types}\|mp3"
types="${types}\|mp2"
types="${types}\|mp4"
types="${types}\|mp4"

types="${types}\|otf"

types="${types}\|png"

types="${types}\|smx"
types="${types}\|swf"

types="${types}\|ttf"
types="${types}\|txt"

types="${types}\|vpk"
types="${types}\|vdf"
types="${types}\|vtf"
types="${types}\|vmf"

types="${types}\|wav"
types="${types}\|woff"

types="${types}\|xspf"

find "$1" -type f -regex ".*\.\($types\)" -exec chmod -x -- {} +
