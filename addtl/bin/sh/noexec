#!/usr/bin/env sh
types=""
types="${types}\|ani"

types="${types}\|bsp"

types="${types}\|c"
types="${types}\|cc"
types="${types}\|cs"
types="${types}\|cfg"
types="${types}\|cnf"
types="${types}\|cpp"
types="${types}\|cur"
types="${types}\|conf"
types="${types}\|config"

types="${types}\|dll"

types="${types}\|fnt"
types="${types}\|fon"

types="${types}\|gif"

types="${types}\|h"
types="${types}\|hh"
types="${types}\|hpp"

types="${types}\|ini"
types="${types}\|inf"

types="${types}\|jpg"
types="${types}\|jpeg"

types="${types}\|mp3"
types="${types}\|mp2"
types="${types}\|mp4"
types="${types}\|mp4"

types="${types}\|otf"

types="${types}\|png"

types="${types}\|smx"
types="${types}\|swf"

types="${types}\|ttf"
types="${types}\|txt"
types="${types}\|tmod"

types="${types}\|vpk"
types="${types}\|vdf"
types="${types}\|vtf"
types="${types}\|vmf"

types="${types}\|wav"
types="${types}\|webm"
types="${types}\|woff"

types="${types}\|xcf"
types="${types}\|xspf"

[ $# -eq 0 ] && ARGS="$PWD" || ARGS=$@

find $ARGS -type f -iregex ".*\.\($types\)" -exec chmod -x -- {} +
