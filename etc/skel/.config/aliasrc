alias claer="clear"
alias bashp=". ~/.bash_profile"

#alias ls="ls -hHNF --color=auto --group-directories-first"
alias l="ls"
alias L="ls -1"
alias la="ls -As"
alias ll="ls -lAs"
alias lS="ls --color=always | sort -k1,1"
alias lA="la --color=always | sort -k1,1"
alias lL="ll --color=always | sort -k1,1"

alias tmux="tmux -2"
alias patch="patch -b"
alias ka="killall"
