    "editor.tokenColorCustomizations":
    {
      "[Darkside Light (rainglow)]":
      {
        "textMateRules":
        [
          { // entity.name.type
            "scope": "entity.name.type",
            "settings": {
              "foreground": "#ff2d6c",
              "fontStyle": "bold"
            }
          },
          { // keyword.operator
            "scope": "keyword.operator",
            "settings": {
              "foreground": "#c5722e",
              "fontStyle": "bold"
            }
          },
          { // keyword.control
            "scope": "keyword.control",
            "settings": {
              "foreground": "#c59d2e",
              "fontStyle": "bold"
            }
          },
          { // keyword.operator
            "scope": "string",
            "settings": {
              "fontStyle": "italic bold"
            }
          },
          { // comment
            "scope": "comment",
            "settings": {
              "fontStyle": "bold"
            }
          },
          { // constant.numeric
            "scope": "constant.numeric",
            "settings": {
              "foreground": "#5522EE",
              "fontStyle": ""
            }
          },
          { // variable
            "scope": "variable",
            "settings": {
              "foreground": "#b412ac",
              "fontStyle": "italic bold"
            }
          },
          { // variable.parameter
            "scope": "variable.parameter",
            "settings": {
              "foreground": "#b412ac",
              "fontStyle": "italic bold"
            }
          },
          { // entity.name.function
            "scope": "entity.name.function",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#b43232"
            }
          },
          { // storage.type
            "scope": "storage.type",
            "settings": {
              "foreground": "#1e00ff",
              "fontStyle": "italic bold"
            }
          },
          { // storage.modifier
            "scope": "storage.modifier",
            "settings": {
              "foreground": "#06a34d",
              "fontStyle": "bold"
            }
          }
        ]
      }
    },
    "[cpp]": {
      "editor.defaultFormatter": "ms-vscode.cpptools"
    }
